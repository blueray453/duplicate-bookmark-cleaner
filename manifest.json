{
  "manifest_version": 2,
  "name": "Duplicate Bookmark Cleaner",
  "version": "1.3",
  "description": "Find and remove duplicate bookmarks in a browser tab",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Duplicate Bookmark Cleaner",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "tab/tab.html",
    "tab/tab.css",
    "tab/tab.js"
  ]
}